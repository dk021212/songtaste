<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="blackRightPadding" format="dimension"></attr>

    <declare-styleable name="BlackSlidingMenu">
        <attr name="blackRightPadding"></attr>
    </declare-styleable>

    <attr name="icon_src" format="reference"></attr>
    <attr name="color" format="color"></attr>
    <attr name="text" format="string"></attr>
    <attr name="text_size" format="dimension"></attr>

    <declare-styleable name="ChangeColorIconWithText">
        <attr name="icon_src"></attr>
        <attr name="color"></attr>
        <attr name="text"></attr>
        <attr name="text_size"></attr>
    </declare-styleable>
    
    <attr name="position">
        <enum name="left_top" value="0"></enum>
        <enum name="left_bottom" value="1"></enum>
        <enum name="right_top" value="2"></enum>
        <enum name="right_bottom" value="3"></enum>
    </attr>
    <attr name="radius" format="dimension"></attr>
    
    <declare-styleable name="ArcMenu">
        <attr name="position"></attr>
        <attr name="radius"></attr>
    </declare-styleable>

</resources>